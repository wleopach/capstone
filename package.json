{
  "homepage": "https://wleopach.github.io/capstone",
  "name": "capstone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^3.2.2",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fortawesome/free-brands-svg-icons": "^6.7.1",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@testing-library/user-event": "^13.5.0",
    "formik": "^2.4.6",
    "framer-motion": "^11.12.0",
    "next-themes": "^0.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.0.1",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "yup": "^1.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "gh-pages": "^6.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@ark-ui/react/(.*)$": [
        "<rootDir>/node_modules/@ark-ui/react/dist/components/$1/index.cjs",
        "<rootDir>/node_modules/@ark-ui/react/dist/components/$1.cjs",
        "<rootDir>/node_modules/@ark-ui/react/dist/providers/$1/index.cjs",
        "<rootDir>/node_modules/@ark-ui/react/dist/providers/$1.cjs"
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(proxy-compare|@zag-js|@ark-ui|@chakra-ui)/)"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  }
}
